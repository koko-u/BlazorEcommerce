{
  "version": "1.0",
  "defaultProvider": "jsdelivr",
  "libraries": [
    {
      "library": "bootstrap@5.2.2",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "LICENSE",
        "README.md",
        "dist/css/bootstrap-grid.css",
        "dist/css/bootstrap-grid.css.map",
        "dist/css/bootstrap-grid.min.css",
        "dist/css/bootstrap-grid.min.css.map",
        "dist/css/bootstrap-reboot.css",
        "dist/css/bootstrap-reboot.min.css",
        "dist/css/bootstrap-utilities.css",
        "dist/css/bootstrap-utilities.css.map",
        "dist/css/bootstrap-utilities.min.css",
        "dist/css/bootstrap-utilities.min.css.map",
        "dist/css/bootstrap.css",
        "dist/css/bootstrap.css.map",
        "dist/css/bootstrap.min.css",
        "dist/css/bootstrap.min.css.map",
        "dist/js/bootstrap.bundle.js",
        "dist/js/bootstrap.bundle.js.map",
        "dist/js/bootstrap.bundle.min.js",
        "dist/js/bootstrap.bundle.min.js.map"
      ]
    },
    {
      "library": "open-iconic@1.1.1",
      "destination": "wwwroot/lib/open-iconic/",
      "files": [
        "FONT-LICENSE",
        "ICON-LICENSE",
        "README.md",
        "font/css/open-iconic-bootstrap.css",
        "font/css/open-iconic-bootstrap.min.css",
        "font/css/open-iconic.css",
        "font/css/open-iconic.min.css",
        "font/fonts/open-iconic.eot",
        "font/fonts/open-iconic.otf",
        "font/fonts/open-iconic.svg",
        "font/fonts/open-iconic.ttf",
        "font/fonts/open-iconic.woff"
      ]
    }
  ]
}